N = int(input("Quantos números você irá digitar: "))

vet = [0 for x in range(N)]
soma = 0
Media = 0

for i in range(N):
  vet[i] = float(input("digite um número: "))
  soma = soma + vet[i]
Media = soma / N


print(" A lista de numeros digitados é de: ", vet)
print(" A soma é de: ", soma)
print(" A média é de: ", Media)
